<form id= "apptSubmit">
    <div class="row">
    <div id="appointment-name" class="input-field col s12 m6">
        <select  id="select-name" class="browser-default">
          <option value="">Select Customer</option>
          {{#each customers as |customer|}}
          <option  id="name-option" value="">{{customer_first_name}} {{customer_last_name}}</option>
          {{/each}}
        </select>
    </div>

    <div id="appointment-time" class="input-field col s12 m6">
        <select class="browser-default" id="select-time">
          <option id="time-option"value="" >Choose Appointment Time</option>
          <option value="9:00">9:00</option>
          <option value="10:00">10:00</option>
          <option value="11:00">11:00</option>
          <option value="12:00">12:00</option>
          <option value="13:00">13:00</option>
          <option value="14:00">14:00</option>
          <option value="15:00">15:00</option>
          <option value="16:00">16:00</option>
          <option value="17:00">17:00</option>
        </select>      
      </div>
  </div>

  <button id="addAppt" style="margin-top: 50px;" class="btn-small waves-effect waves-light" type="submit" name="apptSubmit">Submit
      <i class="material-icons right">send</i>
  </button>

</form>

<script src="/js/addAppt.js"></script>
     